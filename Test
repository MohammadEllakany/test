        `);

text3.remove() // After pick text then remove

      

      var text2 = $(this).find('.card-body div:last-child');

        $(this).find('.card-body').before(`<h5 style="color:#666666;text-align:center;font-weight:bold;font-family:serif;transform:translate(0,-30%); margin-top:0;"> `+ text2.text()+`</h5>


        `);
text2.remove() // After pick text then remove  
      

        

      

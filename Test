function myFunction(e) {
  var copyText = e.target.previousElementSibling;
  copyText.select();
  copyText.setSelectionRange(0, 99999);
  var isDisabled = $(e.target).hasClass("disabled-btn");
  if (!isDisabled) {
    document.execCommand("copy");
    alert("Copied the text: " + copyText.value);
  }
  return false;
}

$("card").ready(function(){
  $("input[value='Action']").replaceWith("<p4>The coupon has already been activated.Just order something...</p4>");

    
  

   $("button").on("click", function(event) {

    event.preventDefault();

    $(this).addClass("disabled-btn");

    $(this).hide();

    var link = $("<a>").attr("href", "https://couponproviders.blogspot.com/2023/02/tips-for-saving-money.html").attr("target", "_blank").text("Just order something from here");

    $(this).parent().append(link);

    link.css("background","#FFB3CB").css("font-family","serif").css("font-weight","bold")
.css("color","#fff").css("text-align", "center").css("text-decoration","none").css("text-align", "center");

});
});
   $(document).ready(function () {

    $("#tb thead, #tb tfoot").remove(); //Remove element

    $("#tb").find('td').removeAttr("align"); // Remove attribute

    

    $('#tb').replaceWith($('#tb').html()

        .replace(/<tbody/gi, "<div id='tb' class='row row-cols-2 row-cols-md-3 row-cols-lg-4 g-3' ")

        .replace(/<tr/gi, "<div class='col'> <div class='card'> <div class='card-body text-center' ")

        .replace(/<\/tr>/gi, "</div></div></div>")

        .replace(/<td/gi, "<div")

        .replace(/<\/td>/gi, "</div>")

        .replace(/<\/tbody/gi, "<\/div")

    );

    // each loop for card layout

    $("#tb .card").each(function() {

      

        

        // Change product style

        
var img2Path = $(this).find('.card-body div:last-child');

        $(this).find('.card-body').before(`

            <div class= "ratio ratio-4x3">

                <img src="`+ img2Path.text()+`" class="card-img-top  p-0 mx-auto start-0 end-2" alt="..." style="max-width:330px;height: auto;

;transform:translate(0,0); margin:0;">

                  

            </div>

            

            

        

        `);

        img2Path.remove() // After pick text then remove
        
     
   var imgPath = $(this).find('.card-body div:last-child');

        $(this).find('.card-body').before(`

            <div class= "ratio ratio-4x3">

                <img src="`+ imgPath.text()+`" class="card-img-top  p-0 mx-auto start-0 end-2" alt="..." style="max-width:330px; height: auto;transform:translate(0,-30%); margin:0;">

                  

            </div>

            

            

        

        `);

        imgPath.remove() // After pick text then remove

         
var text3 = $(this).find('.card-body div:last-child');

